{
  "name": "web-template",
  "dependencies": {
    "jsfxr": "1.2.2"
  },
  "devDependencies": {
    "@babel/core": "7.25.2",
    "@babel/preset-flow": "7.24.7",
    "babel-plugin-syntax-hermes-parser": "0.23.1",
    "clean-css": "5.3.3",
    "ect-bin": "1.4.1",
    "flow-bin": "0.241.0",
    "hermes-eslint": "0.23.0",
    "html-minifier": "4.0.0",
    "roadroller": "2.1.0",
    "standard": "17.1.0",
    "vite": "5.3.4",
    "vite-bundle-visualizer": "1.2.1",
    "vite-plugin-babel": "1.2.0"
  },
  "standard": {
    "envs": [
      "browser"
    ],
    "globals": [
      "$Diff",
      "$Exact",
      "$Exports",
      "$KeyMirror",
      "$Keys",
      "$NonMaybeType",
      "$ReadOnlyArray",
      "$ReadOnly",
      "$Rest",
      "$TupleMap",
      "$Values",
      "Class",
      "gStateMachine",
      "gStateStack",
      "gTextures"
    ],
    "parser": "hermes-eslint",
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "scripts": {
    "analyse": "cd src && vite-bundle-visualizer --config ../vite.config.mjs --template sunburst",
    "build": "USE_RR_CONFIG=1 vite build --config ./vite.config.mjs --emptyOutDir --outDir ../dist src",
    "demo": "cd docs/ && python3 -m http.server",
    "dev": "vite --config ./vite.config.mjs --port 1234 src",
    "dev:phone": "vite --config ./vite.config.mjs --host 192.168.1.234 --port 1234 src",
    "format": "standard src",
    "preview": "vite preview --port 1234"
  }
}
